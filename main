#  python run_app.py
#  python main


import sys
import importlib

if __name__ == "__main__":
    try:
        eccg_module = importlib.import_module('Ccode3DGen_Hun')
        if hasattr(eccg_module, 'main'):
            eccg_module.main()
        else:
            print("Ccode3DGen_Hun.pyd does not have a main() function.")
    except Exception as e:
        print(f"Failed to run Ccode3DGen_Hun.pyd: {e}")



